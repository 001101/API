{
  "name": "hc-api",
  "description": "Human Connection API",
  "version": "0.0.3",
  "homepage": "",
  "main": "server",
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "JB",
    "email": "jb@visualjerk.de"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "server",
    "test": "test/"
  },
  "engines": {
    "node": ">= 6.0.0",
    "yarn": ">= 0.18.0"
  },
  "scripts": {
    "clear": "rm -Rf tmp && rm -Rf public/uploads",
    "test": "npm run eslint && npm run mocha",
    "eslint": "eslint server/. test/. --config .eslintrc.json",
    "start": "concurrently 'mongod' 'wait-on tcp:27017 && NODE_ENV=production node server/'",
    "start-win": "concurrently \"mongod\" \"wait-on tcp:27017 &&SET NODE_ENV=production&& node server/\"",
    "dev": "npm run clear && concurrently 'mongod' 'wait-on tcp:27017 && NODE_ENV=development nodemon --inspect server/'",
    "dev-win": "npm run clear && concurrently \"mongod\" \"wait-on tcp:27017&&SET NODE_ENV=development&& nodemon --inspect server/\"",
    "dev-noseed": "concurrently \"mongod\" \"wait-on tcp:27017 && nodemon --inspect server/\"",
    "mocha": "mocha test/ --recursive"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "compression": "^1.7.1",
    "concurrently": "^3.5.0",
    "cors": "^2.8.4",
    "crypto": "^1.0.1",
    "dauria": "^2.0.0",
    "email-templates": "^3.1.5",
    "feathers": "^2.2.3",
    "feathers-authentication": "^1.3.0",
    "feathers-authentication-hooks": "^0.1.5",
    "feathers-authentication-jwt": "^0.3.2",
    "feathers-authentication-local": "^0.4.4",
    "feathers-authentication-management": "^1.0.0",
    "feathers-blob": "^1.3.1",
    "feathers-configuration": "^0.4.2",
    "feathers-errors": "^2.9.2",
    "feathers-hooks": "^2.1.1",
    "feathers-hooks-common": "^3.10.0",
    "feathers-mailer": "^2.0.0",
    "feathers-memory": "^1.3.1",
    "feathers-mongodb-fuzzy-search": "^1.1.0",
    "feathers-mongoose": "^5.1.2",
    "feathers-rest": "^1.8.0",
    "feathers-seeder": "^1.0.10",
    "feathers-socketio": "^2.0.0",
    "fs-blob-store": "^5.2.1",
    "fs-extra": "^4.0.2",
    "handlebars": "^4.0.11",
    "handlebars-layouts": "^3.1.4",
    "helmet": "^3.9.0",
    "html-excerpt": "^0.1.0",
    "mongoose": "^4.12.4",
    "multer": "^1.3.0",
    "node-sass": "^4.5.3",
    "raven": "^2.2.1",
    "serve-favicon": "^2.4.5",
    "shortid": "^2.2.8",
    "slug": "^0.9.1",
    "wait-on": "^2.0.2",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "eslint": "^4.9.0",
    "mocha": "^4.0.1",
    "nodemon": "^1.12.1",
    "request": "^2.83.0",
    "request-promise": "^4.2.2"
  }
}
