{
  "name": "hc-api",
  "description": "Human Connection API",
  "version": "0.0.1",
  "homepage": "",
  "main": "server",
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "JB",
    "email": "jb@visualjerk.de"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "server",
    "test": "test/"
  },
  "engines": {
    "node": ">= 6.0.0",
    "yarn": ">= 0.18.0"
  },
  "scripts": {
    "test": "npm run eslint && npm run mocha",
    "eslint": "eslint server/. test/. --config .eslintrc.json",
    "start": "concurrently 'mongod' 'wait-on tcp:27017 && NODE_ENV=production node server/'",
    "dev": "concurrently 'mongod' 'wait-on tcp:27017 && NODE_ENV=development nodemon --inspect server/'",
    "dev-noseed": "concurrently 'mongod' 'wait-on tcp:27017 && nodemon --inspect server/'",
    "mocha": "mocha test/ --recursive"
  },
  "dependencies": {
    "body-parser": "^1.17.2",
    "compression": "^1.6.2",
    "concurrently": "^3.4.0",
    "cors": "^2.8.3",
    "feathers": "^2.1.3",
    "feathers-authentication": "^1.2.4",
    "feathers-authentication-hooks": "^0.1.4",
    "feathers-authentication-jwt": "^0.3.1",
    "feathers-authentication-local": "^0.3.4",
    "feathers-authentication-management": "^0.4.0",
    "feathers-configuration": "^0.4.1",
    "feathers-errors": "^2.8.1",
    "feathers-hooks": "^2.0.1",
    "feathers-hooks-common": "^3.5.1",
    "feathers-mailer": "^1.0.6",
    "feathers-mongoose": "^5.1.0",
    "feathers-rest": "^1.7.3",
    "feathers-seeder": "^1.0.8",
    "feathers-socketio": "^2.0.0",
    "helmet": "^3.6.1",
    "html-excerpt": "^0.1.0",
    "jade": "^1.11.0",
    "mongoose": "^4.10.5",
    "serve-favicon": "^2.4.3",
    "shortid": "^2.2.8",
    "slug": "^0.9.1",
    "wait-on": "^2.0.2",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "eslint": "^4.0.0",
    "mocha": "^3.4.2",
    "nodemon": "^1.11.0",
    "request": "^2.81.0",
    "request-promise": "^4.2.1"
  }
}
